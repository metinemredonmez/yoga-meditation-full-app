{
  "name": "yoga-app-backend",
  "version": "1.0.0",
  "description": "Express + TypeScript + Prisma backend for a yoga app",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsx watch src/index.ts",
    "codex": "ts-node codex-cli.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "db:setup": "yarn prisma:migrate && yarn prisma:seed",
    "db:reset": "yarn prisma:reset && yarn prisma:seed",
    "docker:build": "docker build -t yoga-app-backend .",
    "docker:run": "docker run -p 3000:3000 yoga-app-backend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json}\"",
    "ci": "yarn type-check && yarn lint && yarn test",
    "prepare": "husky install || true"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "yoga",
    "backend"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@elastic/elasticsearch": "^9.2.0",
    "@prisma/client": "^5.18.0",
    "@sentry/node": "^10.29.0",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "agora-access-token": "^2.0.4",
    "amqplib": "^0.10.9",
    "apple-signin-auth": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "firebase-admin": "^13.6.0",
    "googleapis": "^167.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "openai": "^5.23.1",
    "otplib": "^12.0.1",
    "pdfkit": "^0.17.2",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^9.0.0",
    "rrule": "^2.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^13.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/hpp": "^0.2.4",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prisma": "^5.18.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "5.5.4",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
